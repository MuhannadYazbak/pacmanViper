<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.MainController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="200.0" fitWidth="800.0" pickOnBounds="true">
                     <image>
                        <Image url="@../../../../Downloads/pacman.jpg" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </top>
         <center>
            <VBox prefHeight="380.0" prefWidth="656.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="startGameBtn" lineSpacing="5.0" mnemonicParsing="false" onAction="#startGame" prefHeight="39.0" prefWidth="112.0" text="Start Game">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="lastGamesBtn" mnemonicParsing="false" onAction="#LastGames" prefHeight="30.0" prefWidth="106.0" text="Last Games">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="questionsBtn" mnemonicParsing="false" onAction="#questions" prefHeight="50.0" prefWidth="100.0" text="Question Wizard" wrapText="true">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets left="350.0" right="350.0" />
               </padding>
               <BorderPane.margin>
                  <Insets bottom="10.0" top="10.0" />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
